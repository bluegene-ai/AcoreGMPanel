/config/generated/*
!/config/generated/.gitkeep

# Cache files
/storage/cache/*
!/storage/cache/.gitkeep

# IP geolocation database (not committed)
/storage/ip_geo/*.mmdb
!/storage/ip_geo/.gitkeep

# Logs
/storage/logs/*
!/storage/logs/.gitkeep

# Composer dependencies
/vendor/

# Local composer phar (use scripts/install-deps.ps1 to download as needed)
/composer.phar

# Environment & IDE files
/.env
/.env.*
/.idea/
/.vscode/
/.DS_Store
Thumbs.db

# Node / build artifacts
/node_modules/
/public/assets/build/
